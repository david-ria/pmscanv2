# Supabase Configuration (read-only credentials)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Storage Configuration
STORAGE_BUCKET=your-bucket-name
CSV_PATH_PREFIX=exports/

# External Dashboard API
DASHBOARD_API_URL=https://your-dashboard-api.com/measurements
DASHBOARD_API_KEY=your-api-key-here

# Rate Limiting
MAX_REQUESTS_PER_SECOND=10
MAX_CONCURRENT_REQUESTS=5

# Polling Configuration
POLL_INTERVAL_MS=60000
PROCESS_BATCH_SIZE=100

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY_MS=1000
BACKOFF_MULTIPLIER=2

# Logging
LOG_LEVEL=info

# Health Server
HEALTH_PORT=3000

# Database
SQLITE_PATH=./data/robot-state.db

# Sensor Mapping
SENSOR_MAP_PATH=./data/sensor_map.csv